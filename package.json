    "lodash-es": "^4.17.21",
    "tv4": "^1.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@types/auth0-js": "^9.14.5",
    "@types/chai": "^4.2.21",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "copy-webpack-plugin": "^5.0.5",
    "eslint": "^8.38.0",
