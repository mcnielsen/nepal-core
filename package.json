    "jest-junit": "^16.0.0",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "rollup": "^2.56.2",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^6.2.2",
    "typescript": "^4.9.5",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
